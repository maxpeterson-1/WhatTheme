{
    "manifest_version": 3,
    "name": "WhatTheme: Shopify Theme Detector",
    "version": "1.1",
    "description": "One-click detection of a Shopify store's theme.",
    "permissions": [
    "activeTab",
    "scripting"
  ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }